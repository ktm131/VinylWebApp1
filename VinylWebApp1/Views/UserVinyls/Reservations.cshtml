@model IEnumerable<VinylWebApp1.Models.VinylReservation>

@{
    ViewData["Title"] = "Twoje rezerwacje";
}

<h1><img width="37" height="37" style="margin-bottom:7px;" src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcT2aeSka-sRGn18Fg33wH-a19Fv8hxb2k92tSrE0tui6wPikUYzrbNs6MuSvIkrwQeeGlM&usqp=CAU" />Twoje rezerwacje</h1>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.ReservationDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ReturnDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Status)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Vinyl)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DeliveryType)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Return)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Payment)
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ReservationDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ReturnDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Status)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Vinyl.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DeliveryType.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Return)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Payment)
            </td>
        </tr>
        }
    </tbody>
</table>
